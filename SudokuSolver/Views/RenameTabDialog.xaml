    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SudokuSolver.Views">

    <StackPanel VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <TextBox x:Uid="NewTabName" x:Name="NewTabNameTextBox" BeforeTextChanging="NewTabName_BeforeTextChanging" TextChanged="NewTabNameTextBox_TextChanged" Paste="NewTabNameTextBox_Paste" MaxLength="124"/>
    </StackPanel>
</ContentDialog>